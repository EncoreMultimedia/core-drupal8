{
    "name": "EncoreMultimedia/core-drupal8",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Johnny Martin"
        },
        {
            "name": "Garrett Whitehorn"
        },
        {
            "name": "Taylor Cammack"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "dev-master",
        "drupal/admin_toolbar": "^1.19",
        "drupal/antibot": "^1.2",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.8",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.0",
        "drupal/drd_agent": "^3.7",
        "drupal/focal_point": "^1.0@beta",
        "drupal/google_analytics": "^2.1",
        "drupal/menu_block": "^1.4",
        "drupal/menu_trail_by_path": "^1.1",
        "drupal/metatag": "^1.5",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "^1.0",
        "drupal/picture_background_formatter": "1.x-dev",
        "drupal/simple_sitemap": "^2.12",
        "drupal/token": "^1.0",
        "drupal/webform": "^5.0",
        "drush/drush": "^9.0.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.8"
    },
    "suggest": {
        "drupal/address": "^1.0; Provides functionality for storing, validating and displaying international postal addresses.",
        "drupal/draggableviews": "^1.x-dev; allows you to make front-end view rows draggable like those on the back-end",
        "drupal/video_embed_field": "^2.0; Provides field type that accepts YouTube/Vimeo video URLs and generates an embedded video."
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {"php": "5.6"}
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@drupal-scaffold"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@drupal-scaffold"
        ],
        "test": [
          "./vendor/bin/phpunit || true"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
